
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (int i = 0, j = 0; i < 5; i++) {
			for (int n = 0; n < (5 - j); n++) {
				System.out.print((n < j) ? " " : "*");
			}
			if (i < 2)
				j++;
			else
				j--;
			System.out.println();
		}
	}
}
